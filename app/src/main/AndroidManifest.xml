<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.eeepay.zzq.agent_kotlin">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:name="com.eeepay.zzq.APP"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/TranslucentTheme">
        <activity android:name="com.eeepay.zzq.koin.FactoryActivity"/>
        <activity android:name="com.eeepay.zzq.mvp.ui.viewmodel.UserActivity"/>
        <activity android:name="com.eeepay.zzq.mvp.ui.lifecycle.LifecycleTestActivity" />
        <activity android:name="com.eeepay.zzq.mvp.ui.Countdown.CountdownAct" />
        <activity android:name="com.eeepay.zzq.mvp.ui.navigation.NavHostMainAct" />
        <activity android:name="com.eeepay.zzq.mvp.ui.CoroutineActivity" />
        <activity android:name="com.eeepay.zzq.mvp.ui.LoginAct">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity> <!-- <activity android:name="com.eeepay.zzq.mvp.ui.LoginAct"/> -->
        <activity android:name="com.eeepay.zzq.mvp.ui.RxMainAct" />
    </application>

</manifest>